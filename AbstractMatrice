public abstract class Matrice<T> implements IMatrice {
    private int ligne, colonne;
    private T type
	
	public Matrice<T> (int ligne, int colonne, Object o) {
		this.ligne = ligne; 
		this.colonne = colonne;
		this.type = t;
		/*
		if (o instanceof int) 
			this.type = new Integer(o);
 
		if (o instanceof String) 
			this.type = new String(o);

		if (o instanceof Point) 
		    this.type = new Point(o);
		 */
	}

	
	
	
	public abstract getLigne();
	
	public abstract getColonne();
	
	public String ToString (Matrice<T> m) {
		for (int i = 0; i < m.getLigne(); ++i) {
			for (int j = 0; j < m.getColonne(); ++j) {
			    if (j = m.getColonne())
			         system.out.println("E" + m.getIndice(m, i, j) + " ");
				else 
				    system.out("E" + m.getIndice(m, i, j));
			}
		}
	}
	
}
